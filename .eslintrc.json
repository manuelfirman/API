{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "env": {
      "node": true
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }